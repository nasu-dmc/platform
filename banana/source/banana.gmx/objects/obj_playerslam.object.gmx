    while(!(place_meeting(x, y+sign(ysp), obj_solid))) { //until 1 pixel away 
        y += sign(ysp); //keep moving up
    }   
    ysp = 0; //once not 1 pixel, stop
}

x += xsp;
y += ysp; 






